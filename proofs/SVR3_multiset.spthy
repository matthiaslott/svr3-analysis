   SCClientIn( $cid, $sid2, mul((~b++koprf2), hpw) ),
   SCClientIn( $cid, $sid3, mul((~b++koprf3), hpw) )
   ]
  --[ ClientReconstructPhase2( $cid, ~pw ) ]->
   [
   SCClientOut( $cid, $sid1,
                sign(h(mul(~b, h(~pw))),
                     KDF(h(<~pw, mul((koprf1++koprf2++koprf3), hpw)>), '1'))
