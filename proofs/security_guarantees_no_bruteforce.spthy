  all-traces
  "∀ koprf #i.
    (Secret( 'koprf', koprf ) @ #i) ⇒ (¬(∃ #j. K( koprf ) @ #j))"
/*
guarded formula characterizing all counter-examples:
"∃ koprf #i.
  (Secret( 'koprf', koprf ) @ #i) ∧ ∃ #j. (K( koprf ) @ #j)"
*/
simplify
solve( Secret( 'koprf', koprf ) @ #i )
  case client_register
  solve( Client_0( $cid, ~pw, ~secret ) ▶₀ #i )
    case client_initialise
    solve( !Server( $sid1 ) ▶₇ #i )
      case create_server
      solve( !Server( $sid2 ) ▶₈ #i )
        case create_server
        solve( !Server( $sid3 ) ▶₉ #i )
          case create_server
          solve( splitEqs(0) )
            case split
            solve( !KU( (~koprf1⊕~koprf2⊕~koprf3) ) @ #vk )
              case c_xor_case_1
              by sorry
            next
              case c_xor_case_2
              solve( !KU( ~koprf1 ) @ #vk.2 )
                case adversary_recv_case_1
                solve( !KU( (~koprf2⊕~koprf3) ) @ #vk.2 )
                  case c_xor
                  solve( !KU( ~koprf3 ) @ #vk.3 )
                    case adversary_recv_case_1
                    solve( !KU( ~koprf2 ) @ #vk.4 )
                      case adversary_recv_case_1
                      solve( SCServerIn( 'adv', $server, <$cid.1, hpw> ) ▶₁ #vr.6 )
                        case server_in_case_1
