rule (modulo E) server_register:
   [
   !Server( $sid ),
   SCServerIn( $client, $sid, <$cid, koprf_share, s_enc_share, kvfy> )
   ]
  -->
   [ !Server_1( $sid, $cid, koprf_share, s_enc_share, kvfy ) ]

